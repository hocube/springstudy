package ex02_DI;

import ex01_nonDI.DAO;

public class Service {
	private DAO dao;

	// 기본 생성자
	public Service() {
	}

	// 생성자를 이용해서 dao 주입
	public Service(DAO dao) {
		// 항상 전역변수로 빼기
		this.dao = dao;
	}

	// setter 이용 방법
	public DAO getDao() {
		return dao;
	}

	public void setDao(DAO dao) {
		this.dao = dao;
	}

	// 실제 실행하고자 하는 메서드
	public void biz() {
		dao.prn();
	}
}
