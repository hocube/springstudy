package ex01_nonDI;

public class Service {
	// 방법1 (Non DI 방법)
	/*
	OracleDAO oracleDAO = new OracleDAO();
	MySQLDAO mySQLDAO = new MySQLDAO();
	
	// 실제 실행하고자 하는 메서드
	public void biz() {
		 oracleDAO.prn();
		// mySQLDAO.prn();
	}
	*/
	// 여기까지 방법1
	
	
	// 방법2 
	// 클래스를 자료형으로 활용
	private DAO dao;
	
	// 기본 생성자
	public Service() {}
	
	// 생성자를 이용해서 dao 주입
	public Service(DAO dao) {
		// 항상 전역변수로 빼기
		this.dao = dao;
	}
}
